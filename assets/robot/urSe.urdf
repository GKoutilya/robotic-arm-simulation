<?xml version="1.0" ?>
<robot name="urSe">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
      <origin xyz="0 0 0.05"/>
    </visual>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="link1">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.2"/>
      </geometry>
      <origin xyz="0 0 0.1"/>
    </visual>
  </link>

  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="link2">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.2"/>
      </geometry>
      <origin xyz="0 0 0.1"/>
    </visual>
  </link>

  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0.2"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="link3">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.2"/>
      </geometry>
      <origin xyz="0 0 0.1"/>
    </visual>
  </link>

  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0 0.2"/>
    <axis xyz="1 0 0"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="link4">
    <visual>
      <geometry>
        <cylinder radius="0.025" length="0.15"/>
      </geometry>
      <origin xyz="0 0 0.075"/>
    </visual>
  </link>

  <!-- Joint 5 -->
  <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0.15"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="link5">
    <visual>
      <geometry>
        <cylinder radius="0.025" length="0.1"/>
      </geometry>
      <origin xyz="0 0 0.05"/>
    </visual>
  </link>

  <!-- Joint 6 -->
  <joint name="joint6" type="revolute">
    <parent link="link5"/>
    <child link="gripper"/>
    <origin xyz="0 0 0.1"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-3.14" upper="3.14" velocity="1.5"/>
  </joint>
  <link name="gripper">
    <visual>
      <geometry>
        <box size="0.05 0.02 0.02"/>
      </geometry>
      <origin xyz="0 0 0.02"/>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>
</robot>
